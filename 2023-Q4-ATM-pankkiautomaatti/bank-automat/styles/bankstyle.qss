    border-radius: 8px;
}

#plus, #minus {
    background-color: #dfdfdf;
    border-style: solid;
    border-width: 1px;
    border-color: #0d0d0d;
    border-radius: 8px;
}

#N0:hover, #N1:hover, #N2:hover, #N3:hover, #N4:hover, #N5:hover, #N6:hover, #N7:hover, #N8:hover, #N9:hover {
    background-color: #efefef;
}

#plus:hover, #minus:hover {
    background-color: #efefef;
}

#N0:pressed, #N1:pressed, #N2:pressed, #N3:pressed, #N4:pressed, #N5:pressed, #N6:pressed, #N7:pressed, #N8:pressed, #N9:pressed, #plus:pressed, #minus:pressed {
    background-color: #876ff9;
    color: #100640;
}
